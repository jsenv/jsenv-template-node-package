{
  "name": "@jsenv/template-node-package",
  "version": "1.0.0-alpha",
  "description": "Package description",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/jsenv-template-node-package"
  },
  "engines": {
    "node": ">=13.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./main.js",
      "require": "./dist/commonjs/main.cjs"
    }
  },
  "module": "main.js",
  "main": "dist/commonjs/main.cjs",
  "files": [
    "/dist/",
    "/src/",
    "/main.js"
  ],
  "scripts": {
    "start-exploring": "node ./script/start-exploring/start-exploring.js",
    "test": "node ./script/test/test.js",
    "test-with-coverage": "npm run test -- --coverage",
    "test-js-transform": "node ./script/test/test-js-transform.js",
    "test-compile-server": "node ./script/test/test-compile-server.js",
    "test-execute": "node ./script/test/test-execute.js",
    "test-execute-test-plan": "node ./script/test/test-execute-test-plan.js",
    "test-exploring": "node ./script/test/test-exploring.js",
    "test-building-commonjs": "node ./script/test/test-building-commonjs.js",
    "test-building-esmodule": "node ./script/test/test-building-esmodule.js",
    "test-building-global": "node ./script/test/test-building-global.js",
    "test-building-systemjs": "node ./script/test/test-building-systemjs.js",
    "test-launch-browser": "node ./script/test/test-launch-browser.js",
    "test-launch-node": "node ./script/test/test-launch-node.js",
    "eslint-check": "node ./node_modules/eslint/bin/eslint.js .",
    "prettier-format": "node ./script/prettier-format/prettier-format.js",
    "prettier-format-stage": "npm run prettier-format -- --staged",
    "prettier-check": "npm run prettier-format -- --dry-run",
    "upload-coverage": "node ./script/upload-coverage/upload-coverage.js",
    "generate-internal-builds": "node --experimental-top-level-await ./script/generate-internal-builds/generate-internal-builds.js",
    "generate-commonjs-build": "node ./script/generate-commonjs-build/generate-commonjs-build.js",
    "generate-import-map": "node ./script/generate-import-map/generate-import-map.js",
    "dist": "npm run generate-commonjs-build && npm run generate-internal-builds",
    "postinstall": "npm run generate-import-map",
    "prepublishOnly": "node ./script/transform-package/remove-postinstall.js && npm run dist",
    "postpublish": "node ./script/transform-package/restore-postinstall.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@jsenv/assert": "2.2.1",
    "@jsenv/codecov-upload": "3.4.3",
    "@jsenv/core": "17.4.0",
    "@jsenv/eslint-config": "12.8.0",
    "@jsenv/github-release-package": "1.2.2",
    "@jsenv/node-module-import-map": "12.2.2",
    "@jsenv/package-publish": "1.5.1",
    "@jsenv/prettier-check-project": "5.6.1",
    "eslint": "7.15.0",
    "prettier": "2.2.1"
  }
}
